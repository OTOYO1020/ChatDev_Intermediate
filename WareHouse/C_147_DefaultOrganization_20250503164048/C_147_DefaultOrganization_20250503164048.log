def is_honest(possible_honest, testimonies):
    honest_set = set(possible_honest)
    for i in range(len(testimonies)):
        if i in honest_set:
            x, y = testimonies[i]
            if not (x in honest_set and y in honest_set):
                return False
        else:
            # If not honest, their testimonies can be ignored
            continue
    return True
