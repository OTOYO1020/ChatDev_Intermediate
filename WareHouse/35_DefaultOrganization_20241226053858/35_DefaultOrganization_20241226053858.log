            switch (choice) {
                case "1":
                    createGame();
                    break;
                case "2":
                    addPlayersToGame();
                    break;
                case "3":
                    recordScores();
                    break;
                case "4":
                    viewLeaderboard();
                    break;
                case "5":
                    running = false;
                    break;
                default:
                    JOptionPane.showMessageDialog(mainWindow, "Invalid choice. Please try again.");
            }
        }
    }

