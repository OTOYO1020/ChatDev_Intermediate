import heapq
class CardManager:
    def __init__(self, n, a):
        self.n = n
        self.cards = a
    def apply_operations(self, operations):
        operations.sort(key=lambda x: x[1], reverse=True)  # Sort by C_j descending
        for b, c in operations:
