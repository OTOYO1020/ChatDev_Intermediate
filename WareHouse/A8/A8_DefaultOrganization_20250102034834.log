        self.master = master
        self.game_data = GameData()
        self.pack()
        self.create_widgets()
    def create_widgets(self):
        self.label = tk.Label(self, text="Enter a three-digit number:")
        self.label.pack()
        self.entry = tk.Entry(self)
        self.entry.pack()
        self.button = tk.Button(self, text="Guess", command=self.check_guess)
        self.button.pack()
