from collections import deque, defaultdict

class CityGraph:
    def __init__(self, N):
        self.N = N
        self.graph = defaultdict(list)

    def add_road(self, A, B):
        self.graph[A].append(B)
        self.graph[B].append(A)

    def find_shortest_paths(self):
        MOD = 10**9 + 7
        distance = [float('inf')] * (self.N + 1)
        count = [0] * (self.N + 1)
        queue = deque([1])
        distance[1] = 0
        count[1] = 1

        while queue:
