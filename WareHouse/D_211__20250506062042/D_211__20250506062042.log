        distance = {i: float('inf') for i in range(1, self.N + 1)}
        count = {i: 0 for i in range(1, self.N + 1)}
        queue = deque([1])
        distance[1] = 0  # Distance to the starting city is 0
        count[1] = 1     # There is one way to reach the starting city
        while queue:
