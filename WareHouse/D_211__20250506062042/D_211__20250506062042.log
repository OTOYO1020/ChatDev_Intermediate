        try:
            N = int(self.n_entry.get())
            M = int(self.m_entry.get())
            graph = CityGraph(N)

            for entry_a, entry_b in self.road_entries:
                A = int(entry_a.get())
                B = int(entry_b.get())
                graph.add_road(A, B)

