            return 0
        return self.bfs()
    def is_valid_move(self, x, y):
        return 0 <= x < self.N and 0 <= y < self.N and self.board[x][y] == '.'
    def get_possible_moves(self, x, y):
        moves = []
        directions = [(-1, -1), (-1, 1), (1, -1), (1, 1)]
        for dx, dy in directions:
            nx, ny = x, y
            while True:
                nx += dx
                ny += dy
                if not (0 <= nx < self.N and 0 <= ny < self.N):
