                if self.board[nx][ny] == 'P':
                    break  # Stop if we hit a pawn
                if self.board[nx][ny] == '.':
                    moves.append((nx, ny))  # Valid empty square
                else:
                    break  # Stop if we hit any other piece
        return moves
    def bfs(self):
        queue = deque([(self.start[0], self.start[1], 0)])  # (x, y, moves)
