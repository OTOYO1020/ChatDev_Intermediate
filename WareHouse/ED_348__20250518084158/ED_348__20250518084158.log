        lines = self.input_data.splitlines()
        n = int(lines[0].strip())
        edges = []
        for i in range(1, n):
            a, b = map(int, lines[i].strip().split())
            edges.append((a, b))
        C = list(map(int, lines[n].strip().split()))
        return n, edges, C
