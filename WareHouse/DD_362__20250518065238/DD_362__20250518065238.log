            for v, edge_weight in self.adj_list[u]:
                weight = current_weight + edge_weight + self.A[v - 1]  # Total weight to reach vertex v
                if weight < min_weights[v]:
                    min_weights[v] = weight
                    heapq.heappush(priority_queue, (weight, v))
