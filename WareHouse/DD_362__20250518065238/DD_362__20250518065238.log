        min_weights = [float('inf')] * (self.N + 1)
        min_weights[1] = self.A[0]  # Weight of vertex 1
        priority_queue = [(self.A[0], 1)]  # (weight, vertex)

        while priority_queue:
            current_weight, u = heapq.heappop(priority_queue)
