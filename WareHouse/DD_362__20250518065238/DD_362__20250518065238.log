import heapq
class Graph:
    def __init__(self, N, M, A):
        self.N = N
        self.M = M
        self.A = A
        self.adj_list = {i: [] for i in range(1, N + 1)}
    def add_edge(self, u, v, weight):
        '''
        Add an edge to the graph. If the graph is undirected, add both directions.
