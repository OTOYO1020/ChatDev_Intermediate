        min_weights[1] = self.A[0]  # Starting point weight
        priority_queue = [(min_weights[1], 1)]  # (weight, vertex)
        while priority_queue:
            current_weight, u = heapq.heappop(priority_queue)
            if current_weight > min_weights[u]:
