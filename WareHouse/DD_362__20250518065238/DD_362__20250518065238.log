        if len(edges) != M:
            raise ValueError("The number of edges provided does not match M.")
        results = find_minimum_path_weights(N, M, A, edges)
        print("Minimum Path Weights:", ', '.join(map(str, results)))
    except Exception as e:
        print("Input Error:", str(e))
