        visited.add(vertex)
        path.append(self.values[vertex - 1])
        # Check if the current path is non-decreasing
        if len(path) > 1 and path[-1] < path[-2]:
            visited.remove(vertex)
            path.pop()
            return
        # Calculate distinct integers in the path
