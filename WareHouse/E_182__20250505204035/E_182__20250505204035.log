        illuminated = set()
        for A, B in self.bulbs:
            # Include the bulb's own position as illuminated
            illuminated.add((A, B))
            # Illuminate in all four directions
            for direction in [(0, 1), (1, 0), (0, -1), (-1, 0)]:  # right, down, left, up
