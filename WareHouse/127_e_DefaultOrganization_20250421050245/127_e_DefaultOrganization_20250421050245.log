    def calculate_distance_cost(self):
        row_cost = self.calculate_row_cost()
        col_cost = self.calculate_col_cost()
        total_cost = (row_cost + col_cost) % MOD
        return total_cost
    def calculate_row_cost(self):
        total = 0
