        self.size = 15
        self.grid = [[None for _ in range(self.size)] for _ in range(self.size)]
    def display_board(self):
        # Display the current state of the board
        for row in self.grid:
            print(' '.join(['.' if cell is None else cell[0] for cell in row]))
