def is_convex_quadrilateral(A_x, A_y, B_x, B_y, C_x, C_y, D_x, D_y):
    def cross_product(v1, v2):
        return v1[0] * v2[1] - v1[1] * v2[0]
    # Vectors AB, BC, CD, DA
    AB = (B_x - A_x, B_y - A_y)
    BC = (C_x - B_x, C_y - B_y)
    CD = (D_x - C_x, D_y - C_y)
    DA = (A_x - D_x, A_y - D_y)
    # Cross products
