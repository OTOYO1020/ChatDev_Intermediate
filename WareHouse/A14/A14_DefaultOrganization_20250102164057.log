        '''
        self.cursor.execute(f"CREATE TABLE {table_name} ({', '.join(columns)})")
        self.connection.commit()

    def insert_data(self, table_name, data):
        '''
        Inserts data into a table.
        Parameters:
            table_name (str): Name of the table
