import tkinter as tk
from tkinter import messagebox
from tuple_counter import count_valid_tuples

class TupleCounterApp:
    '''
    Class to create the main application window and handle user interactions.
    '''
    def __init__(self, master):
        self.master = master
        master.title("Tuple Counter")

        self.label = tk.Label(master, text="Enter a positive integer N:")
        self.label.pack()

        self.entry = tk.Entry(master)
        self.entry.pack()

        self.calculate_button = tk.Button(master, text="Calculate Tuples", command=self.calculate_tuples)
        self.calculate_button.pack()

        self.result_label = tk.Label(master, text="")
        self.result_label.pack()

    def calculate_tuples(self):
        '''
        Method to calculate valid tuples and update the result label.
        '''
        try:
            N = int(self.entry.get())
            if N <= 0:
                raise ValueError("N must be a positive integer.")
            count = count_valid_tuples(N)
            self.result_label.config(text=f"Number of valid tuples: {count}")
        except ValueError as e:
            messagebox.showerror("Input Error", str(e))

if __name__ == "__main__":
    root = tk.Tk()
    app = TupleCounterApp(root)
    root.mainloop()
```

