import tkinter as tk
from tkinter import messagebox
from tree import Tree

class MainApp:
    def __init__(self, master):
        self.master = master
        self.master.title("Tree Counter Application")
        
        self.label_n = tk.Label(master, text="Enter number of vertices (N):")
