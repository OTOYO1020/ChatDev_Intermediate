        self.W = W
        self.blocks = blocks
        self.Q = Q
        self.queries = queries
        self.grid = [[0] * W for _ in range(N)]
        # Initialize blocks in the grid
        for x, y in blocks:
            if 0 <= x < W and 0 <= y < N:  # Ensure blocks are within bounds
                self.grid[y][x] = 1
    def block_exists(self, block_index: int, time: float) -> str:
