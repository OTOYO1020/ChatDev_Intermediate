from itertools import permutations

class Giant:
    def __init__(self, shoulder_height, head_height):
        self.shoulder_height = shoulder_height
        self.head_height = head_height

def calculate_max_height(giants):
    max_height = 0
    N = len(giants)
    indices = list(range(N))
    
