class Matrix:
    def __init__(self, n, m):
        self.n = n
        self.m = m
        self.matrix = [[0] * m for _ in range(n)]
    def update_columns(self, l, r, x):
        for i in range(self.n):
            for j in range(l, r + 1):
                self.matrix[i][j] += x
    def replace_row(self, i, x):
        if 0 <= i < self.n:
