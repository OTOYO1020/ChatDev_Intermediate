    def replace_row(self, i: int, x: int):
        if 0 <= i < self.n:
            self.matrix[i] = [x] * self.m

    def get_value(self, i: int, j: int):
        if 0 <= i < self.n and 0 <= j < self.m:
            return self.matrix[i][j]
        return None  # Return None for out of bounds access
