                if time_shift_a < time_caps_a:
                    total_time += time_shift_a
                    caps_lock_on = True  # Toggle Caps Lock on
                else:
                    total_time += time_caps_a
                    # Keep Caps Lock off
            else:
                total_time += X  # Just press 'A'
        elif char == 'a':
