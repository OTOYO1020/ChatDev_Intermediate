from typing import List
def max_product(N: int, K: int, A: List[int]) -> int:
    MOD = 10**9 + 7
    # Sort the list A in descending order
    A.sort(reverse=True)
    # Handle the case where K is equal to N
    if K == N:
        product = 1
        for num in A:
            product = (product * num) % MOD
        return product
