        self.colors[(end2, color2)] = end1

    def count_cycles_and_non_cycles(self):
        visited = [False] * self.N
        cycles = 0
        non_cycles = 0

        def dfs(node):
            stack = [(node, -1)]
            count = 0
            edges = 0
            while stack:
                current, parent = stack.pop()
                if not visited[current]:
