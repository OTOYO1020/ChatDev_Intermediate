            cycles, non_cycles = self.graph.count_cycles_and_non_cycles()
            self.result_label.config(text=f"Number of cycles: {cycles}, Number of non-cycles: {non_cycles}")
        except Exception as e:
            messagebox.showerror("Error", str(e))
