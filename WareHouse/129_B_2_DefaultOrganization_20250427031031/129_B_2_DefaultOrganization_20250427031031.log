    N = len(weights)
    total_sum = sum(weights)
    min_difference = float('inf')
    for T in range(1, N):
        S1 = sum(weights[:T])
        S2 = total_sum - S1
        min_difference = min(min_difference, abs(S1 - S2))
    return min_difference

