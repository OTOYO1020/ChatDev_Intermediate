class LineCalculator:
    def calculate_lines(self, points, K):  # Accept K as a parameter
        line_count = defaultdict(set)  # Use a set to store unique points on each line
        valid_lines = 0
        N = len(points)
        if N < 2:
            return 0
        # Check for collinearity
        if N == 2:
