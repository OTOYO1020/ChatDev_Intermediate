class LineCalculator:
    def calculate_lines(self, points, K):  # Accept K as a parameter
        line_count = defaultdict(int)
        valid_lines = 0
        N = len(points)
        if N < 2:
            return 0
        # Check for collinearity
        if N == 2:
            return 1 if K <= 2 else 0
        for i in range(N):
