        if self.n == 0 or (self.n > 1 and len(self.adj_list) == 0):
            return 0
        if start == end:
            return 1
        if self.n > 1 and len(self.adj_list) == 0:  # Check if there are no roads
            return 0
        queue = deque([start])
        distances = {i: float('inf') for i in range(1, self.n + 1)}
        distances[start] = 0
        path_count = {i: 0 for i in range(1, self.n + 1)}
        path_count[start] = 1
        while queue:
