        if self.n == 1:  # Only one city
            return 1
        if start == end:  # Start and end are the same
            return 1
        if len(self.adj_list[start]) == 0:  # Check if there are no roads from the start city
            return 0

        queue = deque([start])
        distances = {i: float('inf') for i in range(1, self.n + 1)}
        distances[start] = 0
