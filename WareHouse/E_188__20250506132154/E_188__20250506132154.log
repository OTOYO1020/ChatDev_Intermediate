class Graph:
    def __init__(self, num_towns):
        self.num_towns = num_towns
        self.adj_list = [[] for _ in range(num_towns)]
    def add_edge(self, u, v):
        if 0 <= u < self.num_towns and 0 <= v < self.num_towns:  # Validate indices
            self.adj_list[u].append(v)
        else:
