    def dfs(self, start, prices, visited):
        stack = [start]
        max_price = prices[start]
        visited[start] = True
        while stack:
            node = stack.pop()
            for neighbor in self.adj_list[node]:
                if not visited[neighbor]:
                    visited[neighbor] = True
                    stack.append(neighbor)
                    max_price = max(max_price, prices[neighbor])
        return max_price
