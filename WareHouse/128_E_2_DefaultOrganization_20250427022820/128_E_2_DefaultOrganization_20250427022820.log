    """
    Merges overlapping or contiguous time intervals.
    """
    if not intervals:
        return []
    intervals.sort(key=lambda x: x[0])  # Sort intervals by start time
    merged = [intervals[0]]
    for current in intervals[1:]:
        last = merged[-1]
        if current[0] <= last[1]:  # Overlapping intervals
