            graph[A].append(B)
            in_degree[B] += 1
        else:
            raise ValueError(f"Constraint ({A}, {B}) is out of bounds. Must be between 1 and {N}.")
    # Use a priority queue to maintain the nodes with zero in-degree
    zero_in_degree = []
    for i in range(1, N + 1):
        if in_degree[i] == 0:
            heapq.heappush(zero_in_degree, i)
