'''
Module to calculate the maximum number of consecutive 'X's in a string after performing operations.
'''

def max_consecutive_Xs(S: str, K: int) -> int:
    max_count = 0
    left = 0
    dot_count = 0

    for right in range(len(S)):
        if S[right] == '.':
            dot_count += 1

        while dot_count > K:
            if S[left] == '.':
                dot_count -= 1
            left += 1

        max_count = max(max_count, right - left + 1)

    return max_count
