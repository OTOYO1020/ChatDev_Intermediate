        while dot_count > K:
            if S[left] == '.':
                dot_count -= 1  # Decrease dot count if we are moving past a '.'
            left += 1  # Move the left boundary to the right
        # Update max_count with the size of the current valid window
        max_count = max(max_count, right - left + 1)
