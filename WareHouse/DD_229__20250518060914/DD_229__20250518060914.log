def max_consecutive_Xs(S: str, K: int) -> int:
    # Check if there are no 'X's in the string
    if 'X' not in S:
        return K  # We can convert up to K '.' to 'X'
    max_count = 0  # Variable to track the maximum number of consecutive 'X's
