            elif guess[i] in self.number:
                blows += 1

        if hits == 3:
            return {"hits": hits, "blows": blows}
        if self.lives == 0:
            return "lose"

        self.reduce_lives()

        return {"hits": hits, "blows": blows}

    def reduce_lives(self):
        '''
        Reduces the number of lives by 1.
        '''
        self.lives -= 1
