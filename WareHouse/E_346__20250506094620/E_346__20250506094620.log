def count_colors(H, W, M, operations):
    '''
    Counts the number of cells painted with each color after processing operations.
    '''
    grid = [[0] * W for _ in range(H)]
    row_colors = [0] * H
    col_colors = [0] * W
    for T_i, A_i, X_i in operations:
