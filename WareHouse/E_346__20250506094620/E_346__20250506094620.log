                    raise ValueError(f"Each operation must contain 3 values, but got {len(op)}.")
                T_i, A_i, X_i = op
                if T_i not in (1, 2):
                    raise ValueError(f"Invalid operation type {T_i}. Must be 1 or 2.")
                if (T_i == 1 and not (1 <= A_i <= H)) or (T_i == 2 and not (1 <= A_i <= W)):
