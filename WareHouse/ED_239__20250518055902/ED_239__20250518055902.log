from typing import List, Tuple

class Tree:
    def __init__(self, N: int, edges: List[Tuple[int, int]], values: List[int]):
        self.N = N
        self.edges = edges
        self.values = values
        self.adj_list = [[] for _ in range(N)]
        self.build_tree()

