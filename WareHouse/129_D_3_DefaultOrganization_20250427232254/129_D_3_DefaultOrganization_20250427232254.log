            count = 0
            for i in range(self.H):
                if self.grid[i][j] == '.':
                    count += 1
                    self.light_count[i][j] += count
                else:
                    count = 0  # Reset count on obstacle
            count = 0
            for i in range(self.H - 1, -1, -1):
                if self.grid[i][j] == '.':
                    count += 1
                    self.light_count[i][j] = max(self.light_count[i][j], count)
                else:
                    count = 0  # Reset count on obstacle

