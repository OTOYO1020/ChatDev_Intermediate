                parity[root_x] = parity[x] ^ parity[y] ^ p
            else:
                parent[root_y] = root_x
                parity[root_y] = parity[x] ^ parity[y] ^ p
                rank[root_x] += 1
        else:
            # Check if the current parity matches
            if (parity[x] ^ parity[y]) != p:
                return False  # Conditions cannot be satisfied
