    rank = [0] * N
    parity = [0] * N  # To store parity information
    def find(x):
        if parent[x] != x:
            orig_parent = parent[x]
            parent[x] = find(parent[x])
            parity[x] ^= parity[orig_parent]  # Update parity based on path compression
        return parent[x]
