        '''
        x, y = position
        return 0 <= x < self.size and 0 <= y < self.size and self.grid[x][y] == 0

    def place_part(self, part, position):
        '''
        Places a dragon part at the specified position in the grid.
        '''
        self.grid[position[0]][position[1]] = part

    def get_grid(self):
        '''
