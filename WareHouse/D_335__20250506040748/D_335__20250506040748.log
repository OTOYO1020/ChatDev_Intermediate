class Grid:
    def __init__(self, n):
        self.size = n
        self.grid = [[0 for _ in range(n)] for _ in range(n)]
        self.current_part = 1
        self.takahashi_position = (n // 2, n // 2)
        self.grid[self.takahashi_position[0]][self.takahashi_position[1]] = 'T'

