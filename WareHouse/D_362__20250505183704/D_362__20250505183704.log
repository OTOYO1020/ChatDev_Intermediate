        min_heap = [(self.A[start - 1], start)]  # (distance, vertex)
        distances = {i: float('inf') for i in range(1, self.V + 1)}
        distances[start] = self.A[start - 1]  # Initialize with the weight of the starting vertex
        while min_heap:
