def count_valid_combinations(N, bulb_conditions):
    valid_count = 0
    total_combinations = 1 << N  # 2^N combinations
    for combination in range(total_combinations):
        if all(is_bulb_lit(combination, bulb) for bulb in bulb_conditions):
