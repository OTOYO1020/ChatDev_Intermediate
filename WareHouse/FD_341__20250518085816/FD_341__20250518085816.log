        edges = [tuple(map(int, edge.split(','))) for edge in self.edges_entry.get().split(';')]
        weights = list(map(int, self.weights_entry.get().split(',')))
        pieces = list(map(int, self.pieces_entry.get().split(',')))
        return N, M, edges, weights, pieces
