        total_operations = 0
        while True:
            any_operation = False
            for x in range(self.N):
                if self.pieces[x] > 0:
                    adjacent_vertices = self.adjacency_list[x]
                    # Filter adjacent vertices based on the weight condition
                    valid_vertices = [v for v in adjacent_vertices if self.weights[v] < self.weights[x]]
