        self.weights = weights
        self.pieces = pieces
        self.adjacency_list = self.build_adjacency_list()
    def build_adjacency_list(self):
        adj_list = {i: [] for i in range(self.N)}
        for u, v in self.edges:
            adj_list[u].append(v)
            adj_list[v].append(u)
        return adj_list
    def max_operations(self):
