        period = self.q[t_j - 1]
        remainder = self.r[t_j - 1]
        if d_j % period == remainder:
            return d_j
        else:
            next_day = d_j + (remainder - (d_j % period) + period) % period
            return next_day
    def display_results(self, results):
        for result in results:
            print(result)
