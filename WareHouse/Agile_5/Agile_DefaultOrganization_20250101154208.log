    def calculate_hits_and_blows(self, guess):
        hits = 0
        blows = 0
        for i, digit in enumerate(guess):
            if digit == self.target_number[i]:
                hits += 1
            elif digit in self.target_number and digit != self.target_number[i]:
                blows += 1
        return hits, blows
