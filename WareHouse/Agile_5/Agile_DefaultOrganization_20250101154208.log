        blows = 0
        for i, digit in enumerate(guess):
            if digit == self.target_number[i]:
                hits += 1
            elif digit in self.target_number:
                blows += 1
        return hits, blows

def main():
    game = HitAndBlowGame()
    game.mainloop()

if __name__ == "__main__":
    main()
