    def combinations(n: int, k: int) -> int:
        if k > n or k < 0:
            return 0
        return modular_arithmetic(factorial(n) * pow(factorial(k), MOD - 2, MOD) * pow(factorial(n - k), MOD - 2, MOD) % MOD)  # Apply modulo here

