from utils import modular_arithmetic
def precompute_factorials(max_n: int, mod: int):
    factorials = [1] * (max_n + 1)
    for i in range(2, max_n + 1):
        factorials[i] = (factorials[i - 1] * i) % mod
    return factorials
def countArrangements(N: int, M: int, K: int) -> int:
    MOD = 10**9 + 7
