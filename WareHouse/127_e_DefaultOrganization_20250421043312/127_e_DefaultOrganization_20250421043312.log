class DistanceCalculator:
    def __init__(self, n, m, k):
        self.n = n
        self.m = m
        self.k = k
        self.MOD = 10**9 + 7
        self.fact = [1] * (n * m + 1)
        self.inv_fact = [1] * (n * m + 1)
