        super().__init__()
        self.app = app
        
        self.result_label = Label(self, text="Max Satisfaction: 0")
        self.result_label.pack()

    def show_result(self, max_satisfaction):
        self.result_label.config(text=f"Max Satisfaction: {max_satisfaction}")
