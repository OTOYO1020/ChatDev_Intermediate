        self.entry_d = tk.Entry(self)
        self.entry_d.grid(row=2, column=1)

        self.calculate_button = tk.Button(self, text="Calculate", command=self.calculate_callback)
        self.calculate_button.grid(row=3, columnspan=2)

    def get_values(self):
