        availableTiles.add(new Tile(4, "Yellow"));
        // Initialize the players
        players = new ArrayList<>();
        players.add(new Player("Player 1"));
        players.add(new Player("Player 2"));
        // Set the current player index to 0
        currentPlayerIndex = 0;
    }
