            count = 1
            # Check in the positive direction
            count += self.count_in_direction(x, y, dx, dy)
            # Check in the negative direction
            count += self.count_in_direction(x, y, -dx, -dy)
            if count >= 5:
                return True
        return False
    def count_in_direction(self, x, y, dx, dy):
