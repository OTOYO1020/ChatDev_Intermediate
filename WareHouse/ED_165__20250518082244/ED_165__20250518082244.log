from typing import List, Tuple
from itertools import combinations

def assign_playing_fields(N: int, M: int) -> List[Tuple[int, int]]:
    # Check constraint
    if M * 2 + 1 > N:
        raise ValueError("Constraint not satisfied: M * 2 + 1 must be <= N")
    
    assignments = []
